    {
      "name": "gdb",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/test/deadlock_detection_test", //可执行文件的路径和文件名称
      "preLaunchTask": "make_test",
      "args": [], //主函数调用时传入的参数
      "cwd": "${workspaceFolder}", //调试时的工作目录
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "为 gdb 启用整齐打印",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description":  "将反汇编风格设置为 Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
    },
  ]
}


